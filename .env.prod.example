# Production Environment Variables
# Copy this to .env.prod and fill in your credentials
# DO NOT commit .env.prod to git - only commit this .example file

# NewsData.io API Key
# Get from: https://newsdata.io/dashboard
NEWSDATA_API_KEY=your_newsdata_api_key_here

# DataForSEO API Credentials
# Get from: https://api.dataforseo.com/
DATAFORSEO_API_KEY=your_dataforseo_api_key_here
DATAFORSEO_API_SECRET=your_dataforseo_api_secret_here

# Production settings
NODE_ENV=production
